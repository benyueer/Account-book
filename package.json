{
  "name": "Account-book",
  "version": "1.0.0",
  "description": "",
  "type": "module",
  "private": true,
  "scripts": {
    "lint": "moon run :lint",
    "lint:fix": "moon run :lint -- --fix",
    "build": "moon run :build",
    "dev": "moon run :dev"
  },
  "packageManager": "pnpm@10.28.0",
  "pnpm": {
    "peerDependencyRules": {
      "allowedVersions": {
        "@nestjs/common": "11.x"
      },
      "ignoreMissing": [
        "webpack",
        "class-validator",
        "class-transformer"
      ]
    },
    "onlyBuiltDependencies": [
      "@nestjs/core",
      "@scarf/scarf",
      "bcrypt"
    ]
  },
  "devDependencies": {
    "@antfu/eslint-config": "^7.2.0",
    "@eslint-react/eslint-plugin": "^2.7.2",
    "@unocss/eslint-plugin": "^66.6.0",
    "eslint": "^9.39.2",
    "eslint-plugin-format": "^1.3.1",
    "globals": "^16.5.0",
    "prettier": "^3.4.2",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.20.0"
  }
}